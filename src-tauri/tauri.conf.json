{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "nanokvm_testing_v2",
  "version": "2.1.0",
  "identifier": "com.gugu.nanokvm-testing-v2",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "NanoKVM Testing V2",
        "width": 1300,
        "height": 810
      } 
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
